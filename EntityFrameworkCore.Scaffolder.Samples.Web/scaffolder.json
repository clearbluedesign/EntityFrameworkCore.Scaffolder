	"Scaffolding": {
		"UsePluralizer": true,
		"UseDataAnnotations": false
	},

	"DbContext": {
		"Base": "DbContext"
	},

	"EntityType": {
		"UseLazyLoading": true,
		"BaseMappings": {
			"Categories": [ "IEmptyContract" ],
			"Customers": [ "IContact<String>" ],
			"Employees": [ "EmptyBase" ],
			"Suppliers": [ "Company<String>", "IContact<String>" ]
		}
	}
}
